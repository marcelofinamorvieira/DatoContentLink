{
  "name": "datocms-visual-editing",
  "version": "0.3.0",
  "description": "DatoCMS visual editing overlays without Vercel dependencies.",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE",
    "LICENSES.md",
    "CHANGELOG.md"
  ],
  "sideEffects": false,
  "scripts": {
    "clean": "rimraf dist",
    "build": "tsc -p tsconfig.json",
    "test": "vitest run",
    "test:watch": "vitest",
    "prepublishOnly": "pnpm run build && pnpm run test"
  },
  "keywords": [
    "datocms",
    "visual-editing",
    "stega",
    "content-link"
  ],
  "author": {
    "name": "Marcelo Finamor Vieira",
    "email": "marcelofinamorvieira@gmail.com"
  },
  "license": "MIT",
  "devDependencies": {
    "@types/jsdom": "^21.1.0",
    "@types/node": "^20.14.10",
    "jsdom": "^24.0.0",
    "rimraf": "^5.0.5",
    "typescript": "^5.5.4",
    "vitest": "^1.6.0"
  },
  "dependencies": {
    "@vercel/stega": "^0.1.2"
  },
  "packageManager": "pnpm@9.6.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/marcelofinamorvieira/DatoContentLink.git"
  },
  "bugs": {
    "url": "https://github.com/marcelofinamorvieira/DatoContentLink/issues"
  },
  "homepage": "https://github.com/marcelofinamorvieira/DatoContentLink#readme"
}
